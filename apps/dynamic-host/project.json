{
    "projectType": "application",
    "sourceRoot": "apps/dynamic-host/src",
    "prefix": "trellisorg",
    "targets": {
        "build": {
            "executor": "@nrwl/angular:webpack-browser",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/apps/dynamic-host",
                "index": "apps/dynamic-host/src/index.html",
                "main": "apps/dynamic-host/src/main.ts",
                "polyfills": "apps/dynamic-host/src/polyfills.ts",
                "tsConfig": "apps/dynamic-host/tsconfig.app.json",
                "inlineStyleLanguage": "scss",
                "assets": [
                    "apps/dynamic-host/src/favicon.ico",
                    "apps/dynamic-host/src/assets"
                ],
                "styles": ["apps/dynamic-host/src/styles.scss"],
                "scripts": [],
                "customWebpackConfig": {
                    "path": "apps/dynamic-host/webpack.config.js"
                }
            },
            "configurations": {
                "production": {
                    "budgets": [
                        {
                            "type": "initial",
                            "maximumWarning": "500kb",
                            "maximumError": "1mb"
                        },
                        {
                            "type": "anyComponentStyle",
                            "maximumWarning": "2kb",
                            "maximumError": "4kb"
                        }
                    ],
                    "fileReplacements": [
                        {
                            "replace": "apps/dynamic-host/src/environments/environment.ts",
                            "with": "apps/dynamic-host/src/environments/environment.prod.ts"
                        }
                    ],
                    "outputHashing": "all",
                    "customWebpackConfig": {
                        "path": "apps/dynamic-host/webpack.prod.config.js"
                    }
                },
                "development": {
                    "buildOptimizer": false,
                    "optimization": false,
                    "vendorChunk": true,
                    "extractLicenses": false,
                    "sourceMap": true,
                    "namedChunks": true
                }
            },
            "defaultConfiguration": "production"
        },
        "serve": {
            "executor": "@nrwl/angular:module-federation-dev-server",
            "configurations": {
                "production": {
                    "browserTarget": "dynamic-host:build:production"
                },
                "development": {
                    "browserTarget": "dynamic-host:build:development"
                }
            },
            "defaultConfiguration": "development",
            "options": {
                "port": 4200,
                "publicHost": "http://localhost:4200"
            }
        },
        "extract-i18n": {
            "executor": "@angular-devkit/build-angular:extract-i18n",
            "options": {
                "browserTarget": "dynamic-host:build"
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "options": {
                "lintFilePatterns": [
                    "apps/dynamic-host/**/*.ts",
                    "apps/dynamic-host/**/*.html"
                ]
            }
        }
    },
    "tags": []
}
