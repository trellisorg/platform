{
    "projectType": "application",
    "sourceRoot": "apps/dynamic-remote/src",
    "prefix": "trellisorg",
    "targets": {
        "build": {
            "executor": "@nrwl/angular:webpack-browser",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/apps/dynamic-remote",
                "index": "apps/dynamic-remote/src/index.html",
                "main": "apps/dynamic-remote/src/main.ts",
                "polyfills": "apps/dynamic-remote/src/polyfills.ts",
                "tsConfig": "apps/dynamic-remote/tsconfig.app.json",
                "inlineStyleLanguage": "scss",
                "assets": [
                    "apps/dynamic-remote/src/favicon.ico",
                    "apps/dynamic-remote/src/assets"
                ],
                "styles": ["apps/dynamic-remote/src/styles.scss"],
                "scripts": [],
                "customWebpackConfig": {
                    "path": "apps/dynamic-remote/webpack.config.js"
                }
            },
            "configurations": {
                "production": {
                    "budgets": [
                        {
                            "type": "initial",
                            "maximumWarning": "500kb",
                            "maximumError": "1mb"
                        },
                        {
                            "type": "anyComponentStyle",
                            "maximumWarning": "2kb",
                            "maximumError": "4kb"
                        }
                    ],
                    "fileReplacements": [
                        {
                            "replace": "apps/dynamic-remote/src/environments/environment.ts",
                            "with": "apps/dynamic-remote/src/environments/environment.prod.ts"
                        }
                    ],
                    "outputHashing": "all",
                    "customWebpackConfig": {
                        "path": "apps/dynamic-remote/webpack.prod.config.js"
                    }
                },
                "development": {
                    "buildOptimizer": false,
                    "optimization": false,
                    "vendorChunk": true,
                    "extractLicenses": false,
                    "sourceMap": true,
                    "namedChunks": true
                }
            },
            "defaultConfiguration": "production"
        },
        "serve": {
            "executor": "@nrwl/angular:webpack-server",
            "configurations": {
                "production": {
                    "browserTarget": "dynamic-remote:build:production"
                },
                "development": {
                    "browserTarget": "dynamic-remote:build:development"
                }
            },
            "defaultConfiguration": "development",
            "options": {
                "port": 4201,
                "publicHost": "http://localhost:4201"
            }
        },
        "extract-i18n": {
            "executor": "@angular-devkit/build-angular:extract-i18n",
            "options": {
                "browserTarget": "dynamic-remote:build"
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "options": {
                "lintFilePatterns": [
                    "apps/dynamic-remote/**/*.ts",
                    "apps/dynamic-remote/**/*.html"
                ]
            }
        },
        "serve-static": {
            "executor": "@nrwl/angular:file-server",
            "defaultConfiguration": "development",
            "options": {
                "buildTarget": "dynamic-remote:build",
                "port": 4201
            },
            "configurations": {
                "development": {
                    "buildTarget": "dynamic-remote:build:development"
                },
                "production": {
                    "buildTarget": "dynamic-remote:build:production"
                }
            }
        }
    },
    "tags": []
}
